<%- include("header") -%>
    <div class="box">
      <div id="heading">
        <h1><%= currentDay %></h1>
      </div>
      <% for(var i=0;i<allItems.length;i++){ %>
        <div class="item">
          <input type="checkbox" />
          <p><%= allItems[i] %></p>
        </div>
        <% } %>
        <form action="/" method="POST">
          <input type="text" name="newItem" placeholder="Enter new item" autocomplete="off"/>
          <button type="submit">+</button>
        </form>
      </div>
<%- include("footer") -%>
